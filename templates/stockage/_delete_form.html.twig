<form method="post" action="{{ path('stockage_delete', {'id': stockage.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ stockage.id) }}">
    <button class="btn btn-danger mt-2">Supprimer</button>
</form>
